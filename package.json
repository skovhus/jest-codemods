{
  "name": "jest-codemods",
  "version": "0.0.2",
  "description": "Codemods for switching test framework to Jest",
  "license": "MIT",
  "repository": "skovhus/jest-codemods",
  "author": {
    "name": "Kenneth Skovhus",
    "email": "kenneth.skovhus@gmail.com",
    "url": "github.com/skovhus"
  },
  "bin": "src/cli/bin.js",
  "engines": {
    "node": ">=4"
  },
  "files": [
    "src"
  ],
  "scripts": {
    "check": "npm run lint && npm run test",
    "clean": "rm -rf lib coverage npm-debug.log",
    "lint": "eslint --fix src",
    "prepublish": "npm run clean && npm run check",
    "test:cov": "npm run test -- --coverage",
    "test:watch": "npm run test -- --watch",
    "test": "jest"
  },
  "keywords": [
    "codemod",
    "jest",
    "tape",
    "migrate",
    "migrations",
    "upgrade",
    "transform",
    "jscodeshift",
    "codemods"
  ],
  "dependencies": {
    "babel-register": "~6.14.0",
    "execa": "~0.4.0",
    "is-git-clean": "~1.0.0",
    "jscodeshift": "~0.3.28",
    "meow": "~3.7.0",
    "update-notifier": "~1.0.2"
  },
  "devDependencies": {
    "babel-jest": "^15.0.0",
    "babel-preset-es2015": "^6.14.0",
    "eslint": "^3.5.0",
    "eslint-config-airbnb-base": "^7.1.0",
    "eslint-plugin-import": "^1.15.0",
    "jest": "^15.1.1"
  },
  "jest": {
    "testEnvironment": "node",
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest"
  }
}
